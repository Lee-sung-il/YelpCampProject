<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger w-100" role="alert"  style="word-break: break-word; overflow-wrap: break-word;">
            <h4 class="alert-heading"><%= err.message%></h4>
            <% if(process.env.NODE_ENV !== 'production') {%>
            <p><%= err.stack%></p>
            <% } %>
        </div>
    </div>
</div>


